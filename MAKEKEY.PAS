Type ScreenType = Array [0..3999] Of Byte;
Var Screen : ScreenType Absolute $B800: 0000;
var sname, bname, code: String;
    regfile:text;
label another;
Begin

  another:Move (ImageData, Screen, 4000);
  gotoxy(1,20);
  textcolor(11);
  writeln('Press ENTER to quit...');
  gotoxy(22,8);
  textcolor(15);
  ReadLn (sname);
  if sname = '' then halt;
  gotoxy(1,20);
  writeln('                       ');
  gotoxy(22,9);
  ReadLn (bname);
  gotoxy(22,10);
  textcolor(9);
  assign(regfile,'PRINT.ME');
  rewrite(regfile);
  writeln(regfile,'Here is your registration code, just run Jez-CFG, and you can enter');
  writeln(regfile,'it. Your BBS name and sysop name must match EXACTLY to the below');
  writeln(regfile,'or your code WILL NOT work. Remember, it''s case sensitive.');
  writeln(regfile);
  writeln(regfile,'BBS Name: '+bname);
  writeln(regfile,'Sysop Name: '+sname);
  writeln(regfile,'Code: '+normalize(encrypt (bname+' '+sname)));
  writeln(regfile);
  writeln(regfile,'Thank you for choosing Zodian Software!');
  close(regfile);
  writeln(normalize(encrypt (bname + ' ' + sname) ) );
  gotoxy(1,20);
  textcolor(3);
{  writeln('Press any key...');
  readkey;
  goto another;}
end.